<% if f.status == 'not-ordered' %>
  <td> <%= I18n.t 'new' %> </td>
<% else %>
  <td> <%= I18n.t f.status %> </td>
<% end %>
<td><%=f.period ?  ((I18n.t f.period.strftime("%B"))+f.period.strftime(" %Y")) : 0%></td>
<td><%= f.partner ? f.partner.name : ''  %></td>
<td><%= f.place ? f.place.name : '' %></td>
<td><%= f.price.to_i.ceil %> руб.</td>
<td><%= f.id  %></td>

