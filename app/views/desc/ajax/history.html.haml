.span9
  #well-cont
    %span
      История
  %table.table.table-bordered.hovered
    %thead
      %tr
        %th Статус
        %th Оператор
        %th Время и дата
        %th Ресторан
        %th Сумма
        %th Номер
        /%th №
    %tbody
      - @logs.each do |l|
        %tr{:class => l.status.findClass,:onclick => "location.href='#{partner_order_path(l.order_id)}'"}
          = render 'log',:l=>l